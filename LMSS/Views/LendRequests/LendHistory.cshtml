<div class="card">
 <h2>Lend/Return History</h2>
  <div class="container">
    <table class="table">
                <thead class="table-dark">
                <tr>
                    <th scope="col">Lend Id</th>
                    <th scope="col">Book Name</th>
                    <th scope="col">Lend Date</th>
                    <th scope="col">Due Date</th>
                    <th scope="col">Fine Amount</th>
                    <th scope="col">Status</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model) {
                <tr>
                    
                    <td>@item.LendId</td>
                    <td>@item.Book.BookTitle</td>
                    <td>@item.LendDate</td>
                    <td>@item.ReturnDate</td>
                    <td>@item.FineAmount</td>
                    <td>@item.LendStatus</td>
                    
                    </tr>
                }
            </tbody>
            </table>
  </div>
</div>